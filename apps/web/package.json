    "react-dom": "19.1.0",
    "react-markdown": "^9.1.0",
    "rehype-katex": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "open-next": "^3.1.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.53.0"
